{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 51,
      "minLength": 4
    },
    "slug": {
      "type": "uid",
      "minLength": 3,
      "maxLength": 110,
      "targetField": "title"
    },
    "description": {
      "type": "text",
      "required": false
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "mug",
        "hoodie",
        "purse",
        "cricket bat"
      ],
      "default": "mug",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Size": {
      "type": "string",
      "maxLength": 7,
      "minLength": 1
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "yellow",
        "red",
        "pink",
        "black",
        "blue",
        "orange"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "avilableQty": {
      "type": "integer",
      "required": true
    }
  }
}
